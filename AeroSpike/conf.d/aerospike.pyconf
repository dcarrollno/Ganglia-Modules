    #value_threshold = 0
    #}
    metric {
    name = "stat_read_reqs_xdr"
    title = "XDR Read Requests"
    value_threshold = 0
    }
    metric {
    name = "stat_write_reqs_xdr"
    title = "XDR Write Reqs"
    value_threshold = 0
    }
    # metric {
    #name = "stat_xdr_pipe_writes"
    #title = "XDR Pipe Writes"
    #value_threshold = 0
    #}
    # metric {
    #name = "stat_xdr_pipe_miss"
    #title = "XDR Pipe Misses"
    #value_threshold = 0
    #}
    # metric {
    #name = "stat_xdr_pipe_writes"
    #title = "XDR Pipe Writes"
    #value_threshold = 0
    #}
    metric {
    name = "migrate_msgs_sent"
    title = "Migrate Messages Sent"
    value_threshold = 0
    }
    metric {
    name = "migrate_msgs_recv"
    title = "Migrate Messages Received"
    value_threshold = 0
    }
